"
An icon cache
"
Class {
	#name : #DockerIconCache,
	#superclass : #Object,
	#classVars : [
		'IconCache'
	],
	#category : #'Docker-Tools-Utilities'
}

{ #category : #icons }
DockerIconCache class >> dockerIcon [
	"Private - Generated method"
	^ self iconCache 
			at: #dockerIcon
			ifAbsentPut:[ Form fromBinaryStream: self dockerIconContents base64Decoded readStream ].
]

