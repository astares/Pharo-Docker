{ #category : #icons }
DockerIconCache class >> dockerIcon [
	"Private - Generated method"
	^ self iconCache 
			at: #dockerIcon
			ifAbsentPut:[ Form fromBinaryStream: self dockerIconContents base64Decoded readStream ].
]

